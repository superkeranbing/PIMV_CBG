clear;

% 假设不同的 del 值为 [0.1, 0.3, 0.5]
dels = [0.1, 0.3, 0.5]; 
Dataname = 'Caltech101_7'; 

figure; 

for del = dels
    csvFilePath = [Dataname,'_del',num2str(del),'_k.mat'];
    data = read(csvFilePath);
    k = data.k;
    acc = data.ACC;
    hold on;
    plot(k, acc, 'LineStyle', '-', 'Marker', 'o', 'DisplayName', ['del = ', num2str(del)]); 
end

xlabel('Nearest neighbor number'); 
ylabel('ACC(%)'); 
legend;